{
  "name": "scss-config-webpack-plugin",
  "description": "Preset for scss webpack configuration",
  "bugs": "https://github.com/merkle-open/webpack-config-plugins/issues",
  "homepage": "https://github.com/merkle-open/webpack-config-plugins/tree/master/packages/scss-config-webpack-plugin",
  "repository": "https://github.com/merkle-open/webpack-config-plugins.git",
  "version": "2.0.3-beta.3",
  "author": "Merkle Inc",
  "license": "MIT",
  "main": "src/index.js",
  "keywords": [
    "sass",
    "scss",
    "css",
    "libsass",
    "webpack",
    "loader"
  ],
  "scripts": {
    "test": "export NODE_OPTIONS=--openssl-legacy-provider && jest --detectOpenHandles --runInBand --verbose",
    "posttest": "tsc",
    "update-snapshots": "export NODE_OPTIONS=--openssl-legacy-provider && jest --detectOpenHandles -u --runInBand",
    "test-watch": "export NODE_OPTIONS=--openssl-legacy-provider && jest --detectOpenHandles --watch --runInBand"
  },
  "peerDependencies": {
    "webpack": ">=4.36.0 < 5"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "css-loader": "^5.2.7",
    "iconfont-webpack-plugin": "file:./packages/iconfont-webpack-plugin-5.0.1-beta.1.tgz",
    "mini-css-extract-plugin": "^1.5.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.3.0",
    "postcss-safe-parser": "^7.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^10.4.1",
    "style-loader": "^2.0.0",
    "sass":"1.49.9"
  },
  "devDependencies": {
    "@types/jest": "23.3.14",
    "@types/node": "20.8.5",
    "@types/webpack": "4.41.27",
    "file-loader": "6.2.0",
    "jest": "23.6.0",
    "jsdom": "16.5.3",
    "rimraf": "3.0.2",
    "ts-essentials": "7.0.1",
    "typescript": "4.2.4",
    "url-loader": "4.1.1",
    "webpack": "4.46.0"
  },
  "jest": {
    "browser": false,
    "testEnvironment": "node",
    "rootDir": "./",
    "watchPathIgnorePatterns": [
      "<rootDir>/test/fixtures"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "fixtures",
      "dist"
    ]
  },
  "engines": {
    "npm": ">=6",
    "node": ">=10"
  }
}
